{
  "name": "mini-vue3",
  "version": "1.0.0",
  "description": "mini-vue3",
  "main": "src/index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:build-serve": "rollup --config rollup.config.js --watch",
    "dev:server": "browser-sync start --server --startPath examples --files dist/** --open local --no-notify",
    "build": "rollup --config rollup.config.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linhuibin98/mini-vue3.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/linhuibin98/mini-vue3/issues"
  },
  "homepage": "https://github.com/linhuibin98/mini-vue3#readme",
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "chokidar": "^3.5.2",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rollup": "^2.62.0"
  },
  "dependencies": {
    "jest": "^27.4.5"
  }
}
